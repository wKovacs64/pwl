{
  "name": "pwl",
  "version": "0.0.0-semantically-released",
  "description": "Static web application to reveal character types in a password",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/pwl.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwl/issues"
  },
  "scripts": {
    "build": "tsc && vite build",
    "check-types": "npm-run-all --silent --parallel check-types:*",
    "check-types:src": "tsc",
    "check-types:cypress": "tsc --project cypress/tsconfig.json",
    "cm": "git-cz",
    "cy:open": "cypress open --config baseUrl=http://127.0.0.1:5173",
    "cy:run": "cypress run --config baseUrl=http://127.0.0.1:4173",
    "dev": "vite",
    "format": "prettier --cache --write .",
    "lint": "eslint .",
    "preview": "vite --host 127.0.0.1 preview",
    "test": "npm run test:e2e:open",
    "test:e2e:open": "start-server-and-test dev 5173 cy:open",
    "test:e2e:run": "start-server-and-test preview 4173 cy:run"
  },
  "private": true,
  "dependencies": {
    "@babel/core": "7.22.5",
    "@babel/parser": "7.22.5",
    "@babel/plugin-syntax-jsx": "7.22.5",
    "@fontsource/nunito": "5.0.3",
    "@fontsource/source-sans-3": "5.0.3",
    "@testing-library/cypress": "9.0.0",
    "@types/lodash": "4.14.195",
    "@types/ms": "0.7.31",
    "@types/node": "18.16.19",
    "@types/node-fetch": "2.6.4",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@vitejs/plugin-react-swc": "3.3.2",
    "@wkovacs64/prettier-config": "3.0.3",
    "@xstate/react": "3.2.2",
    "autoprefixer": "10.4.14",
    "axe-core": "4.7.2",
    "clsx": "1.2.1",
    "cypress": "12.16.0",
    "cypress-axe": "1.4.0",
    "eslint": "8.44.0",
    "eslint-plugin-cypress": "2.13.3",
    "eslint-plugin-wkovacs64": "14.3.1",
    "git-current-commit": "1.1.0",
    "hibp": "13.0.0",
    "history": "5.3.0",
    "lodash": "4.17.21",
    "memoize-one": "6.0.0",
    "mobile-detect": "1.4.5",
    "ms": "2.1.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.10.1",
    "react-router-dom": "6.14.1",
    "semantic-release": "21.0.6",
    "start-server-and-test": "2.0.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6",
    "vite": "4.3.9",
    "vite-plugin-pwa": "0.16.4",
    "workbox-build": "7.0.0",
    "workbox-window": "7.0.0",
    "xstate": "4.38.0"
  }
}
