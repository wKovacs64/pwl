{
  "name": "pwl",
  "version": "0.0.0-semantically-released",
  "description": "Static web application to reveal character types in a password",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/pwl.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwl/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "check-types": "npm-run-all --silent --parallel check-types:*",
    "check-types:src": "tsc",
    "check-types:cypress": "tsc --project cypress/tsconfig.json",
    "clean": "gatsby clean",
    "cm": "git-cz",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "develop": "cross-env GATSBY_HOT_LOADER=fast-refresh gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "preserve": "yarn build",
    "serve": "gatsby serve --port 8000",
    "test": "yarn test:e2e",
    "test:e2e": "start-server-and-test develop 8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop 8000 cy:run",
    "validate": "npm-run-all --silent --parallel lint check-types build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@use-it/event-listener": "0.1.3",
    "@wkovacs64/normalize.css": "8.0.1",
    "@xstate/react": "0.8.1",
    "dotenv": "8.2.0",
    "gatsby": "2.23.4",
    "gatsby-plugin-emotion": "4.3.4",
    "gatsby-plugin-manifest": "2.4.11",
    "gatsby-plugin-netlify": "2.3.5",
    "gatsby-plugin-offline": "3.2.10",
    "gatsby-plugin-react-helmet": "3.3.4",
    "gatsby-plugin-use-dark-mode": "1.1.2",
    "git-current-commit": "1.1.0",
    "hibp": "9.0.0",
    "lodash": "4.17.15",
    "memoize-one": "5.1.1",
    "mobile-detect": "1.4.4",
    "ms": "2.1.2",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-icons": "3.10.0",
    "typeface-nunito": "1.1.3",
    "typeface-source-sans-pro": "0.0.75",
    "unfetch": "4.1.0",
    "use-dark-mode": "2.3.1",
    "xstate": "4.10.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/parser": "7.10.2",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@rckeller/cypress-unfetch": "1.0.1",
    "@testing-library/cypress": "6.0.0",
    "@types/color-string": "1.5.0",
    "@types/cypress-axe": "0.8.0",
    "@types/lodash": "4.14.155",
    "@types/memoize-one": "5.1.2",
    "@types/ms": "0.7.31",
    "@types/node": "12.12.47",
    "@types/node-fetch": "2.5.7",
    "@types/react": "16.9.38",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.0.0",
    "@types/testing-library__cypress": "5.0.5",
    "@wkovacs64/prettier-config": "3.0.0",
    "axe-core": "3.5.5",
    "color-string": "1.5.3",
    "commitizen": "4.1.2",
    "cross-env": "7.0.2",
    "cypress": "4.8.0",
    "cypress-axe": "0.8.1",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "6.8.0",
    "eslint-plugin-cypress": "2.11.1",
    "eslint-plugin-emotion": "10.0.27",
    "eslint-plugin-wkovacs64": "6.1.1",
    "gatsby-plugin-typescript": "2.4.6",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "semantic-release": "17.0.8",
    "start-server-and-test": "1.11.0",
    "typescript": "3.9.5"
  },
  "resolutions": {
    "**/@types/react": "16.9.38",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "cypress": "4.8.0"
  }
}
