{
  "name": "pwl",
  "version": "0.0.0-semantically-released",
  "description": "Static web application to reveal character types in a password",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/pwl.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwl/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "check-types": "tsc",
    "clean": "gatsby clean",
    "cm": "git-cz",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,ts,tsx,html,json,md,yml,yaml}\"",
    "lint": "eslint --ext \".js,.ts,.tsx\" .",
    "preserve": "yarn build",
    "serve": "gatsby serve --port 8000",
    "test": "yarn test:e2e",
    "test:e2e": "start-server-and-test develop 8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop 8000 cy:run",
    "validate": "npm-run-all --silent --parallel lint check-types build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@emotion/core": "10.0.14",
    "@emotion/styled": "10.0.14",
    "@use-it/event-listener": "0.1.3",
    "@wkovacs64/normalize.css": "8.0.1",
    "axios": "0.19.0",
    "dotenv": "8.0.0",
    "gatsby": "2.13.27",
    "gatsby-plugin-emotion": "4.1.2",
    "gatsby-plugin-manifest": "2.2.3",
    "gatsby-plugin-netlify": "2.1.3",
    "gatsby-plugin-offline": "2.2.4",
    "gatsby-plugin-react-helmet": "3.1.2",
    "gatsby-plugin-use-dark-mode": "1.1.2",
    "git-current-commit": "1.1.0",
    "hibp": "7.5.2",
    "lodash": "4.17.14",
    "memoize-one": "5.0.5",
    "mobile-detect": "1.4.3",
    "ms": "2.1.2",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "6.0.0-beta",
    "react-icons": "3.7.0",
    "typeface-nunito": "0.0.72",
    "typeface-source-sans-pro": "0.0.75",
    "use-dark-mode": "2.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/parser": "7.5.5",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@cypress/browserify-preprocessor": "2.1.1",
    "@testing-library/cypress": "4.0.4",
    "@types/color-string": "1.5.0",
    "@types/cypress-axe": "0.4.0",
    "@types/lodash": "4.14.136",
    "@types/memoize-one": "4.1.1",
    "@types/ms": "0.7.30",
    "@types/node": "10.14.13",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-helmet": "5.0.8",
    "@typescript-eslint/eslint-plugin": "1.12.0",
    "@wkovacs64/eslint-config-ts-react": "2.1.1",
    "axe-core": "3.3.0",
    "babel-eslint": "10.0.2",
    "color-string": "1.5.3",
    "commitizen": "3.1.1",
    "cypress": "3.4.0",
    "cypress-axe": "0.5.1",
    "cypress-testing-library": "4.0.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.16.0",
    "eslint-plugin-cypress": "2.6.0",
    "eslint-plugin-emotion": "10.0.14",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jest": "22.10.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "1.6.1",
    "gatsby-plugin-typescript": "2.1.2",
    "husky": "3.0.0",
    "lint-staged": "9.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "semantic-release": "15.13.18",
    "start-server-and-test": "1.9.1",
    "tsify": "4.0.1",
    "typescript": "3.5.3"
  },
  "resolutions": {
    "**/@types/react": "16.8.23"
  }
}
