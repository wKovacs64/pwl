{
  "name": "pwl",
  "version": "0.0.0-semantically-released",
  "description": "Static web application to reveal character types in a password",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/pwl.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwl/issues"
  },
  "scripts": {
    "build": "tsc && vite build",
    "check-types": "npm-run-all --silent --parallel check-types:*",
    "check-types:src": "tsc",
    "check-types:cypress": "tsc --project cypress/tsconfig.json",
    "cm": "git-cz",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "vite",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "husky:install": "husky install",
    "prepare": "npm run --silent husky:install",
    "preserve": "yarn build",
    "serve": "vite preview --port 3000",
    "test": "yarn test:e2e",
    "test:e2e": "start-server-and-test develop 3000 cy:open",
    "test:e2e:ci": "start-server-and-test serve 3000 cy:run",
    "validate": "npm-run-all --silent --parallel lint check-types build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@babel/core": "7.16.0",
    "@babel/parser": "7.16.2",
    "@commitlint/cli": "14.1.0",
    "@commitlint/config-conventional": "14.1.0",
    "@fontsource/nunito": "4.5.0",
    "@fontsource/source-sans-pro": "4.5.0",
    "@testing-library/cypress": "8.0.1",
    "@types/color-string": "1.5.0",
    "@types/lodash": "4.14.176",
    "@types/ms": "0.7.31",
    "@types/node": "16.11.6",
    "@types/react": "17.0.34",
    "@types/react-dom": "17.0.11",
    "@types/react-helmet": "6.1.4",
    "@vitejs/plugin-react": "1.0.7",
    "@wkovacs64/prettier-config": "3.0.0",
    "@xstate/react": "1.6.2",
    "autoprefixer": "10.4.0",
    "axe-core": "4.3.5",
    "color-string": "1.6.0",
    "commitizen": "4.2.4",
    "cypress": "8.7.0",
    "cypress-axe": "0.13.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-wkovacs64": "13.6.1",
    "git-current-commit": "1.1.0",
    "hibp": "11.0.0",
    "history": "5.1.0",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "lodash": "4.17.21",
    "memoize-one": "6.0.0",
    "mobile-detect": "1.4.5",
    "ms": "2.1.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.3.11",
    "prettier": "2.4.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-icons": "4.3.1",
    "react-router-dom": "6.0.0-beta.8",
    "semantic-release": "18.0.0",
    "start-server-and-test": "1.14.0",
    "tailwindcss": "2.2.19",
    "typescript": "4.4.4",
    "unfetch": "4.2.0",
    "vite": "2.6.13",
    "vite-plugin-pwa": "0.11.3",
    "workbox-build": "6.3.0",
    "workbox-window": "6.3.0",
    "xstate": "4.26.0"
  }
}
