{
  "name": "pwl",
  "version": "0.0.0-semantically-released",
  "description": "Static web application to reveal character types in a password",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/pwl.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwl/issues"
  },
  "scripts": {
    "build": "tsc && vite build",
    "check-types": "npm-run-all --silent --parallel check-types:*",
    "check-types:src": "tsc",
    "check-types:cypress": "tsc --project cypress/tsconfig.json",
    "cm": "git-cz",
    "cy:open": "cypress open --config baseUrl=http://localhost:3000",
    "cy:run": "cypress run --config baseUrl=http://localhost:4173",
    "dev": "vite",
    "format": "prettier --write .",
    "lint": "eslint .",
    "husky:install": "husky install",
    "prepare": "npm run --silent husky:install",
    "preview": "vite preview",
    "test": "npm run test:e2e:open",
    "test:e2e:open": "start-server-and-test dev http-get://localhost:3000 cy:open",
    "test:e2e:run": "start-server-and-test preview http-get://localhost:4173 cy:run"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@babel/core": "7.18.6",
    "@babel/parser": "7.18.8",
    "@babel/plugin-syntax-jsx": "7.18.6",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@fontsource/nunito": "4.5.9",
    "@fontsource/source-sans-pro": "4.5.10",
    "@testing-library/cypress": "8.0.3",
    "@types/lodash": "4.14.182",
    "@types/ms": "0.7.31",
    "@types/node": "16.11.45",
    "@types/node-fetch": "2.6.2",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-helmet": "6.1.5",
    "@vitejs/plugin-react": "1.3.2",
    "@wkovacs64/prettier-config": "3.0.2",
    "@xstate/react": "3.0.0",
    "autoprefixer": "10.4.7",
    "axe-core": "4.4.3",
    "clsx": "1.2.1",
    "commitizen": "4.2.4",
    "cypress": "10.3.0",
    "cypress-axe": "1.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.19.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-wkovacs64": "13.14.0",
    "git-current-commit": "1.1.0",
    "hibp": "11.1.0",
    "history": "5.3.0",
    "husky": "8.0.1",
    "lint-staged": "12.5.0",
    "lodash": "4.17.21",
    "memoize-one": "6.0.0",
    "mobile-detect": "1.4.5",
    "ms": "2.1.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.12",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-icons": "4.4.0",
    "react-router-dom": "6.3.0",
    "semantic-release": "19.0.3",
    "start-server-and-test": "1.14.0",
    "tailwindcss": "3.1.6",
    "typescript": "4.7.4",
    "unfetch": "4.2.0",
    "vite": "2.9.14",
    "vite-plugin-pwa": "0.12.3",
    "workbox-build": "6.5.3",
    "workbox-window": "6.5.3",
    "xstate": "4.32.1"
  }
}
