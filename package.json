{
  "name": "pwl",
  "version": "0.0.0-semantically-released",
  "description": "Static web application to reveal character types in a password",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/pwl.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwl/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "check-types": "npm-run-all --silent --parallel check-types:*",
    "check-types:src": "tsc",
    "check-types:cypress": "tsc --project cypress/tsconfig.json",
    "clean": "gatsby clean",
    "cm": "git-cz",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "husky:install": "husky install",
    "prepare": "npm run --silent husky:install",
    "preserve": "yarn build",
    "serve": "gatsby serve --port 8000",
    "test": "yarn test:e2e",
    "test:e2e": "start-server-and-test develop 8000 cy:open",
    "test:e2e:ci": "start-server-and-test serve 8000 cy:run",
    "validate": "npm-run-all --silent --parallel lint check-types build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "11.4.1",
    "@emotion/styled": "11.3.0",
    "@fontsource/nunito": "4.5.0",
    "@fontsource/source-sans-pro": "4.5.0",
    "@use-it/event-listener": "0.1.6",
    "@wkovacs64/normalize.css": "8.0.3",
    "@xstate/react": "1.6.1",
    "dotenv": "10.0.0",
    "gatsby": "3.14.2",
    "gatsby-plugin-emotion": "6.14.0",
    "gatsby-plugin-manifest": "3.14.0",
    "gatsby-plugin-netlify": "3.14.0",
    "gatsby-plugin-offline": "4.14.0",
    "gatsby-plugin-react-helmet": "4.14.0",
    "gatsby-plugin-use-dark-mode": "1.3.0",
    "git-current-commit": "1.1.0",
    "hibp": "11.0.0",
    "lodash": "4.17.21",
    "memoize-one": "5.2.1",
    "mobile-detect": "1.4.5",
    "ms": "2.1.3",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-icons": "4.3.1",
    "unfetch": "4.2.0",
    "use-dark-mode": "2.3.1",
    "xstate": "4.25.0"
  },
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@babel/parser": "7.15.8",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@testing-library/cypress": "8.0.1",
    "@types/color-string": "1.5.0",
    "@types/cypress-axe": "0.8.0",
    "@types/lodash": "4.14.175",
    "@types/memoize-one": "5.1.2",
    "@types/ms": "0.7.31",
    "@types/node": "14.17.25",
    "@types/react": "17.0.29",
    "@types/react-dom": "17.0.9",
    "@types/react-helmet": "6.1.3",
    "@wkovacs64/prettier-config": "3.0.0",
    "axe-core": "4.3.3",
    "babel-preset-gatsby": "1.14.0",
    "color-string": "1.6.0",
    "commitizen": "4.2.4",
    "core-js": "3.18.3",
    "cypress": "8.6.0",
    "cypress-axe": "0.13.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-wkovacs64": "13.5.2",
    "gatsby-plugin-typescript": "3.14.0",
    "husky": "7.0.2",
    "lint-staged": "11.2.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "semantic-release": "18.0.0",
    "start-server-and-test": "1.14.0",
    "typescript": "4.4.4"
  },
  "resolutions": {
    "**/@types/react": "17.0.29",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0"
  }
}
