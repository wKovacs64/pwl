{
  "name": "pwl",
  "version": "0.0.0-semantically-released",
  "description": "Static web application to reveal character types in a password",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/pwl.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwl/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "check-types": "npm-run-all --silent --parallel check-types:*",
    "check-types:src": "tsc",
    "check-types:cypress": "tsc --project cypress/tsconfig.json",
    "clean": "gatsby clean",
    "cm": "git-cz",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "develop": "cross-env GATSBY_HOT_LOADER=fast-refresh gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "preserve": "yarn build",
    "serve": "gatsby serve --port 8000",
    "test": "yarn test:e2e",
    "test:e2e": "start-server-and-test develop 8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop 8000 cy:run",
    "validate": "npm-run-all --silent --parallel lint check-types build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@emotion/core": "10.1.1",
    "@emotion/styled": "10.0.27",
    "@use-it/event-listener": "0.1.5",
    "@wkovacs64/normalize.css": "8.0.1",
    "@xstate/react": "1.0.3",
    "dotenv": "8.2.0",
    "gatsby": "2.26.1",
    "gatsby-plugin-emotion": "4.4.0",
    "gatsby-plugin-manifest": "2.6.0",
    "gatsby-plugin-netlify": "2.5.0",
    "gatsby-plugin-offline": "3.4.0",
    "gatsby-plugin-react-helmet": "3.4.0",
    "gatsby-plugin-use-dark-mode": "1.1.2",
    "git-current-commit": "1.1.0",
    "hibp": "9.0.0",
    "lodash": "4.17.20",
    "memoize-one": "5.1.1",
    "mobile-detect": "1.4.4",
    "ms": "2.1.2",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-icons": "3.11.0",
    "typeface-nunito": "1.1.13",
    "typeface-source-sans-pro": "0.0.75",
    "unfetch": "4.2.0",
    "use-dark-mode": "2.3.1",
    "xstate": "4.14.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/parser": "7.12.5",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@rckeller/cypress-unfetch": "1.0.1",
    "@testing-library/cypress": "7.0.1",
    "@types/color-string": "1.5.0",
    "@types/cypress-axe": "0.8.0",
    "@types/lodash": "4.14.165",
    "@types/memoize-one": "5.1.2",
    "@types/ms": "0.7.31",
    "@types/node": "12.19.4",
    "@types/node-fetch": "2.5.7",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "@types/react-helmet": "6.1.0",
    "@wkovacs64/prettier-config": "3.0.0",
    "axe-core": "4.0.2",
    "color-string": "1.5.4",
    "commitizen": "4.2.2",
    "cross-env": "7.0.2",
    "cypress": "5.6.0",
    "cypress-axe": "0.10.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.13.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-emotion": "10.0.27",
    "eslint-plugin-wkovacs64": "10.0.0",
    "gatsby-plugin-typescript": "2.6.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "semantic-release": "17.2.2",
    "start-server-and-test": "1.11.5",
    "typescript": "4.0.5"
  },
  "resolutions": {
    "**/@types/react": "16.9.56",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0"
  }
}
