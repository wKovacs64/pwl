{
  "name": "pwl",
  "version": "0.0.0-semantically-released",
  "description": "Static web application to reveal character types in a password",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/pwl.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwl/issues"
  },
  "scripts": {
    "build": "tsc && vite build",
    "check-types": "npm-run-all --silent --parallel check-types:*",
    "check-types:src": "tsc",
    "check-types:cypress": "tsc --project cypress/tsconfig.json",
    "cm": "git-cz",
    "cy:open": "cypress open --config baseUrl=http://127.0.0.1:5173",
    "cy:run": "cypress run --config baseUrl=http://127.0.0.1:4173",
    "dev": "vite",
    "format": "prettier --cache --write .",
    "lint": "eslint .",
    "preview": "vite --host 127.0.0.1 preview",
    "test": "npm run test:e2e:open",
    "test:e2e:open": "start-server-and-test dev 5173 cy:open",
    "test:e2e:run": "start-server-and-test preview 4173 cy:run"
  },
  "private": true,
  "dependencies": {
    "@babel/core": "7.21.4",
    "@babel/parser": "7.21.4",
    "@babel/plugin-syntax-jsx": "7.21.4",
    "@fontsource/nunito": "4.5.12",
    "@fontsource/source-sans-pro": "4.5.11",
    "@testing-library/cypress": "9.0.0",
    "@types/lodash": "4.14.192",
    "@types/ms": "0.7.31",
    "@types/node": "18.15.11",
    "@types/node-fetch": "2.6.3",
    "@types/react": "18.0.34",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react-swc": "3.3.0",
    "@wkovacs64/prettier-config": "3.0.3",
    "@xstate/react": "3.2.1",
    "autoprefixer": "10.4.14",
    "axe-core": "4.6.3",
    "clsx": "1.2.1",
    "cypress": "12.9.0",
    "cypress-axe": "1.4.0",
    "eslint": "8.38.0",
    "eslint-plugin-cypress": "2.13.2",
    "eslint-plugin-wkovacs64": "14.1.0",
    "git-current-commit": "1.1.0",
    "hibp": "12.0.1",
    "history": "5.3.0",
    "lodash": "4.17.21",
    "memoize-one": "6.0.0",
    "mobile-detect": "1.4.5",
    "ms": "2.1.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "prettier": "2.8.7",
    "prettier-plugin-tailwindcss": "0.2.7",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.8.0",
    "react-router-dom": "6.10.0",
    "semantic-release": "21.0.1",
    "start-server-and-test": "2.0.0",
    "tailwindcss": "3.3.1",
    "typescript": "4.9.5",
    "vite": "4.2.1",
    "vite-plugin-pwa": "0.14.7",
    "workbox-build": "6.5.4",
    "workbox-window": "6.5.4",
    "xstate": "4.37.1"
  }
}
