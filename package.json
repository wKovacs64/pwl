{
  "name": "pwl",
  "version": "1.1.1-dev.0",
  "description": "Static web application to reveal character types in a password",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/pwl.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwl/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf .cache public",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,json,md,yml}\"",
    "lint": "eslint .",
    "preserve": "yarn build",
    "serve": "gatsby serve",
    "test": "yarn test:e2e",
    "test:e2e": "start-server-and-test develop 8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop 8000 cy:run"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "axios": "0.18.0",
    "emotion": "9.2.10",
    "emotion-server": "9.2.10",
    "gatsby": "2.0.8",
    "gatsby-plugin-emotion": "2.0.5",
    "gatsby-plugin-manifest": "2.0.3",
    "gatsby-plugin-netlify": "2.0.0",
    "gatsby-plugin-offline": "2.0.5",
    "gatsby-plugin-react-helmet": "3.0.0",
    "git-current-commit": "1.1.0",
    "hibp": "7.1.3",
    "lodash.debounce": "4.0.8",
    "memoize-one": "4.0.2",
    "mobile-detect": "1.4.3",
    "ms": "2.1.1",
    "normalize.css": "8.0.0",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-emotion": "9.2.10",
    "react-helmet": "5.2.0",
    "react-icons": "3.1.0",
    "react-key-handler": "1.2.0-beta.3",
    "typeface-nunito": "0.0.54",
    "typeface-source-sans-pro": "0.0.54"
  },
  "devDependencies": {
    "babel-eslint": "10.0.0",
    "color-string": "1.5.3",
    "cypress": "3.1.0",
    "cypress-testing-library": "2.2.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-cypress": "2.0.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "husky": "1.0.0",
    "lint-staged": "7.3.0",
    "prettier": "1.14.3",
    "rimraf": "2.6.2",
    "start-server-and-test": "1.7.1"
  }
}
