{
  "name": "pwl",
  "version": "0.0.0-semantically-released",
  "description": "Static web application to reveal character types in a password",
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/pwl.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwl/issues"
  },
  "scripts": {
    "build": "tsc && vite build",
    "check-types": "npm-run-all --silent --parallel check-types:*",
    "check-types:src": "tsc",
    "check-types:cypress": "tsc --project cypress/tsconfig.json",
    "cm": "git-cz",
    "cy:open": "cypress open --config baseUrl=http://127.0.0.1:5173",
    "cy:run": "cypress run --config baseUrl=http://127.0.0.1:4173",
    "dev": "vite",
    "format": "prettier --write .",
    "lint": "eslint .",
    "husky:install": "husky install",
    "prepare": "npm run --silent husky:install",
    "preview": "vite --host 127.0.0.1 preview",
    "test": "npm run test:e2e:open",
    "test:e2e:open": "start-server-and-test dev http-get://127.0.0.1:5173 cy:open",
    "test:e2e:run": "start-server-and-test preview http-get://127.0.0.1:4173 cy:run"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@babel/core": "7.20.2",
    "@babel/parser": "7.20.3",
    "@babel/plugin-syntax-jsx": "7.18.6",
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@fontsource/nunito": "4.5.11",
    "@fontsource/source-sans-pro": "4.5.11",
    "@testing-library/cypress": "8.0.3",
    "@types/lodash": "4.14.189",
    "@types/ms": "0.7.31",
    "@types/node": "18.11.9",
    "@types/node-fetch": "2.6.2",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/react-helmet": "6.1.5",
    "@vitejs/plugin-react": "2.2.0",
    "@wkovacs64/prettier-config": "3.0.2",
    "@xstate/react": "3.0.1",
    "autoprefixer": "10.4.13",
    "axe-core": "4.5.2",
    "clsx": "1.2.1",
    "commitizen": "4.2.5",
    "cypress": "11.0.1",
    "cypress-axe": "1.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.27.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-wkovacs64": "13.17.0",
    "git-current-commit": "1.1.0",
    "hibp": "11.1.0",
    "history": "5.3.0",
    "husky": "8.0.2",
    "lint-staged": "13.0.3",
    "lodash": "4.17.21",
    "memoize-one": "6.0.0",
    "mobile-detect": "1.4.5",
    "ms": "2.1.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.19",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-icons": "4.6.0",
    "react-router-dom": "6.4.3",
    "semantic-release": "19.0.5",
    "start-server-and-test": "1.14.0",
    "tailwindcss": "3.2.4",
    "typescript": "4.8.4",
    "unfetch": "4.2.0",
    "vite": "3.2.3",
    "vite-plugin-pwa": "0.13.3",
    "workbox-build": "6.5.4",
    "workbox-window": "6.5.4",
    "xstate": "4.34.0"
  }
}
